    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.9",
    "pg": "^8.16.0",
    "pino": "^9.6.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "sequelize": "^6.37.7",

    "supertest": "^7.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",

    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.2",
